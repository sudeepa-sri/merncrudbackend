{
  "name": "super-service",
  "vigour": {
    "service": "super-service",
    "category": "services",
    "userdecay": {
      "$val": 5,
      "description": "minutes unused userdata is kept in memory",
      "parse": "toInt"
    },
    "port": {
      "$val": 10001,
      "description": "port to listen for websocket connections",
      "parse": "toInt",
      "dowhap": "{port}"
    },
    "special": {
      "$val": false,
      "description": "marks this instance as special!"
    }
  }
}
